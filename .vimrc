set tabstop=4
set shiftwidth=4
set expandtab
set smarttab
set smoothscroll
set ai
set wrap
set display+=lastline
filetype plugin on
filetype indent on
highlight EndOfBuffer ctermfg=black ctermbg=black
set backspace=eol,start,indent
set ignorecase
set hlsearch
syntax enable
set linebreak
nnoremap j gj
nnoremap k gk
